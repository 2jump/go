{
  "name": "cropperjs",
  "description": "JavaScript image cropper.",
  "version": "2.0.0-beta",
  "main": "dist/cropper.js",
  "module": "dist/cropper.esm.js",
  "types": "dist/cropper.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "api-extractor": "api-extractor run --local --verbose",
    "build": "npm run tsc && npm run rollup && npm run api-extractor",
    "clean": "del-cli dist temp",
    "release": "npm run clean && npm run build",
    "rollup": "rollup -c ../../rollup.config.js",
    "tsc": "tsc --outDir ./temp --emitDeclarationOnly"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fengyuanchen/cropperjs.git",
    "directory": "packages/cropperjs"
  },
  "keywords": [
    "image",
    "crop",
    "move",
    "zoom",
    "rotate",
    "scale",
    "cropper",
    "cropper.js",
    "image-cropping",
    "image-viewing",
    "image-processing",
    "cropper-element",
    "custom-element",
    "web-component",
    "html",
    "css",
    "javascript",
    "front-end",
    "web"
  ],
  "author": "Chen Fengyuan (https://chenfengyuan.com/)",
  "license": "MIT",
  "bugs": "https://github.com/fengyuanchen/cropperjs/issues",
  "homepage": "https://github.com/fengyuanchen/cropperjs/tree/next/packages/cropperjs/#readme",
  "dependencies": {
    "@cropper/canvas": "^2.0.0-beta",
    "@cropper/constants": "^2.0.0-beta",
    "@cropper/crosshair": "^2.0.0-beta",
    "@cropper/element": "^2.0.0-beta",
    "@cropper/grid": "^2.0.0-beta",
    "@cropper/handle": "^2.0.0-beta",
    "@cropper/image": "^2.0.0-beta",
    "@cropper/selection": "^2.0.0-beta",
    "@cropper/shade": "^2.0.0-beta",
    "@cropper/utils": "^2.0.0-beta",
    "@cropper/viewer": "^2.0.0-beta"
  },
  "publishConfig": {
    "tag": "next"
  }
}
