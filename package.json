{
  "name": "cropperjs",
  "version": "2.0.0-beta.1",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "build": "npm run bootstrap && vuepress build docs",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "dev": "npm run bootstrap && vuepress dev docs",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:css": "stylelint docs/.vuepress/**/*.{css,scss,vue} --fix",
    "lint:js": "eslint . docs/.vuepress --fix",
    "prepare": "husky install",
    "release": "npm run bootstrap && lerna run release && npm run lint && npm test && npm run build && npm run changelog",
    "start": "npm run dev",
    "test": "jest"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@microsoft/api-extractor": "^7.20.1",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-typescript": "^8.3.3",
    "@types/estree": "^0.0.51",
    "@types/jest": "^28.1.2",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "@vuepress/plugin-google-analytics": "^2.0.0-beta.49",
    "@vuepress/plugin-pwa": "^2.0.0-beta.49",
    "@vuepress/plugin-pwa-popup": "^2.0.0-beta.49",
    "@vuepress/plugin-register-components": "^2.0.0-beta.49",
    "@vuepress/plugin-search": "^2.0.0-beta.49",
    "change-case": "^4.1.2",
    "conventional-changelog-cli": "^2.2.2",
    "create-banner": "^2.0.0",
    "cssnano": "^5.1.12",
    "del-cli": "^4.0.1",
    "eslint": "^8.18.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.3",
    "eslint-plugin-vue": "^9.1.1",
    "husky": "^8.0.1",
    "jest": "^28.1.1",
    "jest-environment-jsdom": "^28.1.1",
    "lerna": "^5.1.4",
    "lint-staged": "^13.0.2",
    "postcss": "^8.4.14",
    "postcss-cli": "^9.1.0",
    "postcss-html": "^1.4.1",
    "rgb-hex": "^4.0.0",
    "rollup": "^2.75.6",
    "rollup-plugin-inline-postcss": "^3.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "sass-loader": "^13.0.0",
    "stylelint": "^14.9.1",
    "stylelint-config-recommended-scss": "^6.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-order": "^5.0.0",
    "ts-jest": "^28.0.5",
    "ts-loader": "^9.3.0",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4",
    "vuepress": "^2.0.0-beta.49",
    "vuepress-webpack": "^2.0.0-beta.49"
  }
}
